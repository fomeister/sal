<?xml version="1.0"?>
<SalPlugins xmlns="http://sal.jcu.edu.au/schemas/pluginsConfig">
	<ConfigHelpers>
		<Helper>
			<name>HalHelper</name>
			<class>jcu.sal.plugins.config.hal.HalHelper</class>
		</Helper>
	</ConfigHelpers>
	<ProtocolPlugins>
		<Protocol>
			<name>SALSpot</name>
                        <class>jcu.sal.plugins.protocols.sunspot.SunSPOTProtocol</class>
                        <ConfigPlugin>
                                <HelperName>HalHelper</HelperName>
                                <class>jcu.sal.plugins.protocols.sunspot.HalClient</class>
                        </ConfigPlugin>
		</Protocol>
		<Protocol>
			<name>owfs</name>
			<class>jcu.sal.plugins.protocols.owfs.OWFSProtocol</class>
			<ConfigPlugin>
				<HelperName>HalHelper</HelperName>
				<class>jcu.sal.plugins.protocols.owfs.HalClient</class>
			</ConfigPlugin>
		</Protocol>
		<Protocol>
			<name>SSNMP</name>
			<class>jcu.sal.plugins.protocols.simpleSNMP.SSNMPProtocol</class>
		</Protocol>
		<Protocol>
			<name>PlatformData</name>
			<class>jcu.sal.plugins.protocols.osData.OSDataProtocol</class>
			<ConfigPlugin>
				<HelperName>HalHelper</HelperName>
				<class>jcu.sal.plugins.protocols.osData.HalClient</class>
			</ConfigPlugin>
		</Protocol>
		<Protocol>
			<name>v4l2</name>
			<class>jcu.sal.plugins.protocols.v4l2.V4L2Protocol</class>
			<ConfigPlugin>
				<HelperName>HalHelper</HelperName>
				<class>jcu.sal.plugins.protocols.v4l2.HalClient</class>
			</ConfigPlugin>
		</Protocol>
		<Protocol>
			<name>DUMMY</name>
			<class>jcu.sal.plugins.protocols.dummy.DummyProtocol</class>
		</Protocol>
	</ProtocolPlugins>
	<EndPointPlugins>
		<EndPoint>
			<name>usb</name>
			<class>jcu.sal.plugins.endpoints.UsbEndPoint</class>
		</EndPoint>
		<EndPoint>
			<name>serial</name>
			<class>jcu.sal.plugins.endpoints.SerialEndPoint</class>
		</EndPoint>
				<EndPoint>
			<name>ethernet</name>
			<class>jcu.sal.plugins.endpoints.EthernetEndPoint</class>
		</EndPoint>
				<EndPoint>
			<name>fs</name>
			<class>jcu.sal.plugins.endpoints.FSEndPoint</class>
		</EndPoint>
		<EndPoint>
			<name>pci</name>
			<class>jcu.sal.plugins.endpoints.PCIEndPoint</class>
		</EndPoint>
	</EndPointPlugins>
</SalPlugins>
